alias aa="$EDITOR $HOME/.config/zsh/aliases.zsh"
alias src="source ~/.zshrc"

alias ls="exa -lh --color=never --group-directories-first"
alias la="exa -alh --color=never --group-directories-first"
alias tree="exa --tree --level=2"

alias cat="batcat"

alias kc="kubectl"
alias kx="kubectx"

alias gc="gcloud"
